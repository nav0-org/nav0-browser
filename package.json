{
  "name": "nav0-browser",
  "version": "0.1.0",
  "description": "A minimal, privacy-focused web browser built on Electron",
  "private": true,
  "type": "module",
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "postinstall": "electron-rebuild -f -w better-sqlite3 && electron-rebuild -f -w node-llama-cpp && node patch-scripts/patch-compileLLamaCpp.js",
    "rebuild": "electron-rebuild -f -w better-sqlite3 && electron-rebuild -f -w node-llama-cpp && node patch-scripts/patch-compileLLamaCpp.js",
    "build-node-llama-cpp": "npm rebuild node-llama-cpp --build-from-source && node patch-scripts/patch-compileLLamaCpp.js "
  },
  "keywords": [],
  "author": {
    "name": "Ketan Patil",
    "email": "100.percent.ketan@gmail.com"
  },
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
  
    "@electron-forge/maker-deb": "^7.8.0",
    "@electron-forge/maker-dmg": "^7.8.0",
    "@electron-forge/maker-rpm": "^7.8.0",
    "@electron-forge/maker-squirrel": "^7.8.0",
    "@electron-forge/maker-zip": "^7.8.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.0",
    "@electron-forge/plugin-fuses": "^7.8.0",
    "@electron-forge/plugin-webpack": "^7.8.0",
    "@electron/fuses": "^1.8.0",
    "@electron/rebuild": "^4.0.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/bluebird": "^3.5.42",
    "@types/pdf-parse": "^1.1.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^6.11.0",
    "electron": "35.2.1",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "html-loader": "^5.1.0",
    "node-loader": "^2.1.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "vitepress": "^1.5.0"
